services:
  szpnuw.portal:
    build:
      context: D:\Repo\SZPNUW
      dockerfile: SZPNUW.Portal/Dockerfile
    depends_on:
    - szpnuw.webapi.account
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: szpnuwportal
    ports:
    - 7000:80/tcp
  szpnuw.webapi.account:
    build:
      context: D:\Repo\SZPNUW
      dockerfile: Microservices/SZPNUW.WebAPI.Account/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: szpnuwwebapiaccount
    ports:
    - 7001:80/tcp
version: '3.0'